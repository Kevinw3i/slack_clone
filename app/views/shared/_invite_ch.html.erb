<div class="ch-background absolute w-full h-full top-0 bg-black bg-opacity-25 hidden flex justify-center items-center z-40">
  <div class="px-6 py-4 bg-white shadow-2xl w-4/5 md:w-1/3 h-1/3 rounded relative">
    <div class="flex justify-between">
      <h5 class="font-bold text-xl"><%= t(".invite") %></h5>
      <button class="invite-cancel-btn font-bold text-gray-600 focus:outline-none">X</button>
    </div>
    <span class="text-xs text-gray-600">#<%= @channel.name %></span>
    <%= form_with(url: invite_channel_users_channels_path(@channel.id),method: "POST",local: true) do |f| %>
      <select name="users_channels[name][]" id="user_list" multiple="true" style="width: 100%">
        <%= options_for_select(@workspace_users)%>
      </select>
      <%= f.submit t(".send") ,class:"invite-btn cursor-pointer block font-bold text-gray-700 px-5 py-2 mt-3 ml-auto bg-gray-400 rounded focus:outline-none",disabled: true %>
    <% end %>
    <div class="search-data hidden absolute top:0 bg-gray-100 py-2 inset-x-0 mx-5 border border-gray-500 rounded" >
    </div>
  </div>
</div>