<%= form_with(model: @channel, url:workspace_channels_path(id: @channel.workspace_id)) do |f| %>
  <div class="field">
    <%= f.label :name, "channel name" %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :topic %>
    <%= f.text_field :topic %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <%= f.submit "submit" %>
<% end %>